<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE beans PUBLIC "-//SPRING//DTD BEAN 2.0//EN"
        "http://www.springframework.org/dtd/spring-beans-2.0.dtd">
      <beans> 
       <bean id ="std " class ="com.entity.Student">
 <!-- DI using XML with property-based injection -->
       
       <property name="std_id" value="1"></property>
       <property name ="std_name" value ="ganesh"></property>
       <property name="age" value="23"></property>
       <property name="con" ref="c"></property>
        </bean>
        <bean id="c" class ="com.entity.Country">
        <property name="c_id" value="91" ></property>
        <property name="name" value="India"></property>
        
        </bean>
        </beans>
        